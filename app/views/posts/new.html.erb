<% content_for :head do %>
  
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
  <%= javascript_import_module_tag "post" %>
 
  
<% end %>
<% content_for :body do %>

<% end %>


<%= render "form", post: @post %>
<div style="display:grid;grid-auto-flow:column;margin-left: inherit;margin-top: 20px;margin-right: 300px;">
<button class="genButton" id="start-camera">Start Camera</button>
<video id="video" width="320" height="240" autoplay></video>
<button class="genButton" id="click-photo">Click Photo</button>
<canvas id="canvas" width="320" height="240"></canvas>
<button class="genButton" id="upload">Upload</button>
</div>
 