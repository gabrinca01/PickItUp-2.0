<% content_for :body do %>
    <script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '2366483176861220',
      xfbml      : true,
      version    : 'v16.0'
    });
    FB.AppEvents.logPageView();
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "https://connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>
<% end %>
<div id= "timeline">
	
	
	<%= simple_form_for @post, html: { class: "post form" },action: "/posts" do |f| %>

  <%= f.input :written_text, input_html: { autofocus: true } %>
  <%= f.input :images, multiple: true ,value: @post.images,required: true ,html: { style: "" }%>
  <%= f.submit 'Post',class: "btn btn--secondary" %>
<% end %>
	<%= render @timeline_posts %>
	
</div>
<div id = "awards">
	<%=render  @awards %>
	
</div>