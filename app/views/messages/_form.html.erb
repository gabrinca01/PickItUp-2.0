<% content_for :head do %>
  <%= javascript_import_module_tag "flush" %> 
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
<% end %>

<%= turbo_frame_tag "new message",target: "_top" do %>
<div class="form-group msg-form">
  <%= form_with(model: [@challenge ,@message], remote: true, class: "d-flex" ,id: "f" ) do |f| %>
    <%= f.text_field :msg_txt, class:"autoclear", id: 'chat-text',class: "form-control msg-content", autocomplete: 'off' %>
    <%= f.submit  'Send' ,id: "sub" , data: { disable_with: false }, class: "genButton", style:"margin:7px" %>
  <% end %>
</div>
<%end%>


